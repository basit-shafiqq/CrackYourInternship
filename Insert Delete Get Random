import java.util.*;

class RandomizedCollection {

    private List<Integer> list;
    private Map<Integer, Set<Integer>> indexMap;
    private Random rand;

    public RandomizedCollection() {
        list = new ArrayList<>();
        indexMap = new HashMap<>();
        rand = new Random();
    }

    public boolean insert(int val) {
        // Check if the value is new
        boolean isNewValue = !indexMap.containsKey(val);

        // If value is new, create a new set for it
        if (isNewValue) {
            indexMap.put(val, new HashSet<>());
        }

        // Add the index of the new value to the set
        indexMap.get(val).add(list.size());

        // Add the value to the list
        list.add(val);

        return isNewValue;
    }

    public boolean remove(int val) {
        // Check if the value is present and has indices
        if (!indexMap.containsKey(val)) {
            return false;
        }

        // Get an arbitrary index of the value to be removed
        int removeIndex = indexMap.get(val).iterator().next();
        indexMap.get(val).remove(removeIndex);

        // Get the last element in the list
        int lastElement = list.get(list.size() - 1);

        // If the element to be removed is not the last element, move the last element to the removeIndex
        if (removeIndex < list.size() - 1) {
            list.set(removeIndex, lastElement);
            indexMap.get(lastElement).remove(list.size() - 1);
            indexMap.get(lastElement).add(removeIndex);
        }

        // Remove the last element from the list
        list.remove(list.size() - 1);

        // If no more indices are associated with the value, remove the entry from the map
        if (indexMap.get(val).isEmpty()) {
            indexMap.remove(val);
        }

        return true;
    }

    public int getRandom() {
        return list.get(rand.nextInt(list.size()));
    }
}

/**
 * Your RandomizedCollection object will be instantiated and called as such:
 * RandomizedCollection obj = new RandomizedCollection();
 * boolean param_1 = obj.insert(val);
 * boolean param_2 = obj.remove(val);
 * int param_3 = obj.getRandom();
 */
