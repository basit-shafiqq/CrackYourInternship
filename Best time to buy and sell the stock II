class Solution {
    public int maxProfit(int[] prices) {
        
        int maxProfit = 0;
        int currentStock = prices[0];

        for(int i=1;i<prices.length;i++){
         
            if(prices[i]>currentStock){
                maxProfit+=prices[i]-currentStock;
            }
            currentStock = prices[i];
            
        }
        return maxProfit;
    }
}
