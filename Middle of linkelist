
public class Solution
{
    public static Node findMiddle(Node head)
    {
        Node fast = head;
        Node slow = head;

        while(fast!=null && fast.next!=null){
            fast = fast.next;
            slow=slow.next;
            if(fast.next!=null){
                fast = fast.next;
            }
        }
        return slow;
    }
}
